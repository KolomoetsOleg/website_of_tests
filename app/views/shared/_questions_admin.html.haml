%table
  %tr
    %th
      %h4 Вопрос
    %th
      %h4 Тип вопроса
    %th
    %th
  - @quests.each do |quest|
    %tr
      %td
        %h5= quest.title
      %td
        %h5
          = quest.tip_vop
      %td
        = link_to "Редактировать", "#", :class => "update", :name => quest.id , :value => quest.tip_vop
      %td
        = link_to "Удалить", admin_quest_path(quest), method: :delete, data: { confirm: 'Are you sure?' }