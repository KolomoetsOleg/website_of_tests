<table>
  <tr>
    <th>
      <h4>Вопрос</h4>
    </th>
    <th>
      <h4>Тип вопроса</h4>
    </th>
    <th>

    </th>
    <th>

    </th>
  </tr>
  <% @quests.each do |quest| %>

      <tr>
        <td>
          <h5><%= quest.title %></h5>
        </td>
        <td>
          <h5><%= quest.tip_vop%> </h5>
        </td>
        <td>
          <%= link_to "Редактировать", edit_admin_quest_path(quest) %>
        </td>
        <td>
          <%= link_to "Удалить", admin_quest_path(quest), method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>

      </tr>
  <% end %>

</table>